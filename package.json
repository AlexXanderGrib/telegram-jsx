  "bugs": {
    "url": "https://github.com/AlexXanderGrib/telegram-jsx/issues"
  },
  "homepage": "https://github.com/AlexXanderGrib/telegram-jsx#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
