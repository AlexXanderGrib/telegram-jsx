{
  "name": "telegram-jsx",
  "version": "1.0.0",
  "description": "TSX/JSX runtime for telegram bots",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "exports":{
    ".": {
      "types": "./global.d.ts"
    },
    "./runtime": {
      "import": "./runtime.mjs",
      "require": "./runtime.js",
      "types": "./runtime.d.ts"
    } 
  },
